{
	"compilerOptions": {
		"lib": ["ESNext", "DOM"],
		"module": "node16",
		"target": "es2022",

		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node16",
		"outDir": "dist",
		"sourceMap": true,
		"declaration": true
	}
}
